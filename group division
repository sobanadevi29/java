import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc= new Scanner(System.in);
        int a= sc.nextInt();
        int b= sc.nextInt();
        int studentinteam = a%b;
        int remaining = a/b;
        System.out.println("The number of students in each team is " + remaining + " and left out is " + studentinteam);
        
   
    }
}
