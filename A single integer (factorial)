import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int fact=1;
        for(int i=1;i<=num;i++)
        {
            fact=fact*i;
            
        }
        int sum = 0;
        while (fact > 0) {
            sum += fact % 10;
            fact /= 10;
        }
        System.out.println(sum);
}
}
