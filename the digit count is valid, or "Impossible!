import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
        long totalDigits = sc.nextLong();
        long pages = 0;
        long digitsPerPage = 1;
        long blockCount = 9;

        while (totalDigits > 0) {
            long digitsNeeded = blockCount * digitsPerPage;
            if (totalDigits >= digitsNeeded) {
                totalDigits -= digitsNeeded;
                pages += blockCount;
                digitsPerPage++;
                blockCount *= 10;
            } else {
                
                if (totalDigits % digitsPerPage == 0) {
                    pages += totalDigits / digitsPerPage;
                    totalDigits = 0;
                } else {
                    System.out.println("Impossible!");
                    return;
                }
            }
        }
        System.out.println(pages);
    }
}
