import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String num = sc.nextLine();
        
        if (num.contains("4") || num.contains("7") ){ 
            System.out.println("YES");
        } 
        else {
            System.out.println("NO");
        }
    }
}
