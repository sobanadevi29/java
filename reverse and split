package day7;

public class problem1 {

	public static void main(String[] args) {
		StringBuffer sb=new StringBuffer("hello world");
		System.out.println(sb);
		sb.reverse();
		System.out.println(sb);
		String str = sb.toString();

		String[]word=str.split(" ");
		for(String var:word) {
			System.out.println(var);
		}

	}

}
