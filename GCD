package day4;
import java.util.Scanner;
public class problem6 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int num2=sc.nextInt();
		  int gcd = 0;
		for(int i=1;i<=num && i<=num2;i++) {
			if (num % i == 0 && num2 % i == 0) 
			{
				gcd= i;
				}
			
			}
		System.out.println(gcd);
	}

}
