package day8;

public class constructor{
	int age;
	String name;
	constructor(int age,String name){
		this.age=age;
		this.name=name;
	}
	void display() {
		System.out.println(age +" "+name);
	}
		public static void main(String[] args) {
			constructor obj=new constructor(5,"sobana");
			obj.display();
		}
	
}
