package day5;
import java.util.Scanner;
public class palindrome {

	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		int num = sc.nextInt();
		int rev=0;
		int temp1=num;
		while(num>0) {
			int temp=num%10;
			rev =rev*10+temp;
			num=num/10;
		}
		if(temp1==rev) {
			System.out.println("it is palindrome");
		}
		else {
			System.out.println("not a palindrome");
		}

	}

}
