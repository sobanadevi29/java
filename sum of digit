import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt(); 

        int firstDigit = num / 10;   
        int secondDigit = num % 10;  
        int sum = firstDigit + secondDigit;

        System.out.println(sum);

        sc.close();
    }
}
